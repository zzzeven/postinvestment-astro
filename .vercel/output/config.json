{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/ai/chat/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/me/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/files/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/files/([^/]+?)/tags/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/files/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/files/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/folders/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/folders/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/tags/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/tags/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}